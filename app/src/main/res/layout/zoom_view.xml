<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageButton
        android:id="@+id/buttonPlus"
        android:layout_width="@dimen/zoom_view_button_size"
        android:layout_height="@dimen/zoom_view_button_size"
        android:layout_marginTop="@dimen/zoom_view_button_margin"
        android:layout_marginLeft="@dimen/zoom_view_button_margin"
        android:layout_marginStart="@dimen/zoom_view_button_margin"
        android:layout_marginRight="@dimen/zoom_view_button_margin"
        android:layout_marginEnd="@dimen/zoom_view_button_margin"
        android:foreground="@drawable/zoom_view_button_foreground"
        android:src="@drawable/plus"
        android:translationZ="@dimen/zoom_view_button_translation_z"
        tools:ignore="ContentDescription, UnusedAttribute" />

    <ImageButton
        android:id="@+id/buttonMinus"
        android:layout_width="@dimen/zoom_view_button_size"
        android:layout_height="@dimen/zoom_view_button_size"
        android:layout_margin="@dimen/zoom_view_button_margin"
        android:foreground="@drawable/zoom_view_button_foreground"
        android:src="@drawable/minus"
        android:translationZ="@dimen/zoom_view_button_translation_z"
        tools:ignore="ContentDescription, UnusedAttribute" />

</merge>